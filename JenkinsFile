pipeline
        {
                agent any
                stages {
                        stage('One') {
                        steps {
                                echo 'Hi, this is Suganthi from NCS'
                                }
                        }
                        stage('Two')
                        {
                        steps {
                                echo 'Hello Wolrd'
                                }
                        }
			stage('Three')
			{
			steps {
			       echo 'Hello World Stage3'
			       }
			 }
			 stage('Four')
			 {
			 steps {
			 	echo 'Hello from stage 4'
				}
		        }

                }

        }


